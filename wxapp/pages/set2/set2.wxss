.ba-radio{
  -webkit-appearance:none;
appearance:none;
margin-right:5px;
outline:0;
border:1px solid #D1D1D1;
background-color:#ffffff;
border-radius:50%;
width:22px;
height:22px;
position:relative;

}
.ba-form-cell-select>view{
  display: inline-block;
  width:100rpx;
  height:100rpx;
  border-radius: 50%;
  /* border:1px solid #e1e1e1; */
  text-align: center;
  line-height: 3.5;
  font-size: 30rpx;
  margin:10rpx 20rpx;
  background: #e1e1e1;
}
.ba-form-cell-select>view.checked{
   color:#fff;
   background: #ee816d;
}
.ba-submit{
  position: absolute;
  bottom:-60rpx;
  left:50%;
  margin-left:-40rpx;
  /* background: #fff; */
}
.xgb-gou{
  color:#e1e1e1;
  font-size:80rpx;
  background:#fff;
  border-radius:50%;
}
.canClick{
  background:#fff;
  color:#ee816d;
}